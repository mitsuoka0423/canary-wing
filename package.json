{
  "name": "dev-launcher",
  "version": "1.0.3",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "start": "tsc && webpack && NODE_ENV=DEV electron ./dist/index.js",
    "tsc": "tsc",
    "watch": "tsc -w",
    "build:render": "tsc && webpack",
    "package:mac": "tsc && webpack && electron-builder --mac --x64",
    "package:windows": "tsc && webpack && electron-builder --win --x64",
    "package:linux": "tsc && webpack && electron-builder --linux --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmitsuoka0423/dev-launcher.git"
  },
  "keywords": [],
  "author": "Takahiro Mitsuoka <tmitsuoka0423@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tmitsuoka0423/dev-launcher/issues"
  },
  "homepage": "https://github.com/tmitsuoka0423/dev-launcher#readme",
  "dependencies": {
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^4.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "^12.12.6",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "css-loader": "^3.6.0",
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0",
    "eslint": "^7.3.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "build": {
    "productName": "dev-launcher",
    "appId": "dev-launcher.tmitsuoka0423.com",
    "directories": {
      "output": "./product"
    },
    "files": [
      "./dist/**/*",
      "./index.html",
      "./icon/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": "msi"
    }
  }
}
